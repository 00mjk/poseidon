      "environment": {
        "POSEIDON_PUBLIC_SESSIONS": "1",
        "RABBIT": "True",
        "RABBIT_EXCHANGE": "",
        "RABBIT_HOST": "messenger",
        "RABBIT_QUEUE": "True",
        "RABBIT_QUEUE_NAME": "task_queue",
        "RABBIT_ROUTING_KEY": "task_queue",
